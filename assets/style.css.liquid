/******************
HOVER IMAGES STATE
******************/

figure.global-brands {
  margin: 0.5%;
  padding: 0;
}

.sunburst {
  background-image: -moz-linear-gradient( 90deg, rgb(237,50,25) 0%, rgb(8,25,248) 100%);
  background-image: -webkit-linear-gradient( 90deg, rgb(237,50,25) 0%, rgb(8,25,248) 100%);
  background-image: -ms-linear-gradient( 90deg, rgb(237,50,25) 0%, rgb(8,25,248) 100%);
}

figure.sunburst img {
    opacity: 1.0;
}

figure.sunburst:hover img {
    opacity: 0.698;
    -webkit-transition: all .3s;
    -moz-transition: all .3s;
    transition: all .3s;
}

figure.sunburst figcaption::before {
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 10px;
    background: #fff;
    content: '';
    -webkit-transform: translate3d(0,10px,0);
    transform: translate3d(0,10px,0);
}

figure.sunburst h2, figure.sunburst p, figure.sunburst span {
    position: absolute;
    bottom: 0;
    left: 30px;
    right: 30px;
    line-height: 3;
    opacity: 0;
    color: #fff;
    text-align: left;
}

figure.sunburst:hover h2, figure.sunburst:hover p, figure.sunburst:hover span {
    position: absolute;
    bottom: 0;
    left: 30px;
    right: 30px;
    line-height: 3;
    opacity: 1;
    color: #fff;
    text-align: center;
}

figure.sunburst:hover span {
    font-size: 8em;
}

@media only screen and (min-width: 769px) and (max-width: 999px) {

    figure.sunburst:hover h2, figure.sunburst:hover p, figure.sunburst:hover span {
        position: absolute;
        bottom: 0;
        left: 5%;
        right: 5%;
        line-height: 2;
        opacity: 1;
        color: #fff;
        text-align: center;
    }

    figure.sunburst:hover h2 {
        font-size: 1.8em;
    }

    figure.sunburst:hover span {
        font-size: 3.0em;
        top: 10%;
    }
}

figure.sunburst:hover figcaption::before,
figure.sunburst:hover h2,
figure.sunburst:hover p {
    opacity: 1;
    -webkit-transform: translate3d(0,0,0);
    transform: translate3d(0,0,0);
    -webkit-transition: opacity 0.2s, -webkit-transform 0.2s;
    transition: opacity 0.2s, transform 0.2s;
}


