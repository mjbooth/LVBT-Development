  <script src="{{ 'custom.modernizr.min.js' | asset_url }}" ></script>
  <script src='//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js'></script>
  <script src='//ajax.googleapis.com/ajax/libs/jqueryui/1.10.3/jquery-ui.min.js' async></script>
  <script src="{{ 'api.jquery.js' | shopify_asset_url }}" async></script>
  <script src="//load.sumome.com/" data-sumo-site-id="8a69768f5f612b20a7c2f3013b9953bc9fe5a13062cb4bec21373eb96ad0f4b5" async="async"></script>
  <script src="{{ 'trunk.min.js' | asset_url }}" ></script>
  <script type="text/javascript" src="https://cdn.ywxi.net/js/1.js" async></script>


  {% if template contains 'customers' %}
  {{ 'customer.css' | asset_url | stylesheet_tag }}
  {{ 'shopify_common.js' | shopify_asset_url | script_tag }}
  {{ 'customer_area.js'  | shopify_asset_url | script_tag }}
  {% endif %}

  <script>
  var twitterID = '{{ settings.twitter_username }}',
  general_external_links_enabled = {{ settings.general-external-links-enabled }},
  general_scroll_to_active_item = {{ settings.general-scroll-to-active-item }},
  shop_url = '{{ shop.url }}',
  shop_money_format = '{{ shop.money_format | strip_html | replace: "'", '"' | strip }}',
  product_zoom_enabled = {{ settings.product-zoom-enabled }},
  product_modal_enabled = {{ settings.product-modal-enabled }};
  </script>

  <script>(function() {
    var _fbq = window._fbq || (window._fbq = []);
    if (!_fbq.loaded) {
      var fbds = document.createElement('script');
      fbds.async = true;
